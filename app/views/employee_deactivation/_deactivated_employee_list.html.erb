<table>
  <thead>
  <tr>
    <th>Employee Number</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Location</th>
    <th>Deactivation Date</th>
  </tr>
  </thead>
  <tbody>
  <% @employees.each do |employee| %>
    <tr>
      <td><%= employee.employee_number %></td></td>
      <td><%= employee.first_name %></td></td>
      <td><%= employee.last_name %></td></td>
      <td><%= employee.location_name %></td></td>
      <td><%= DateHelpers::format(employee.deactivation_date) %></td></td>
    </tr>
  <% end %>
  </tbody>
</table>